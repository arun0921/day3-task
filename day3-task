FOR;

const obj = {
    "name": "Arun",
    "age": 22,
    "hobbies": ["riding", "swimming", "trucking"],  "address": ["1/49f17a,gudalur,niligirs,tamil nadu"]
    
    
    };
    
    for (let prop in obj) {  if (obj.(prop)) {
    console.log(`${prop}: ${obj[prop]}`);
    }
    }
    
    FOR In;

    const obj = {
        "name": "Arun",  "age": 22,
        "hobbies": ["riding", "swimming", "trucking"],  "address": ["1/49f17a,gudalur,niligirs,tamil nadu"]
        
        for (let prop in obj) {
        if (obj.hasOwnProperty(prop)) {
        console.log(`${prop}: ${obj[prop]}`);
        }
        }

        FOR Off;

        
        const obj = {
            "name": "Arun",  "age": 22,
            "hobbies": ["riding", "swimming", "trucking"],  "address": ["1/49f17a,gudalur,niligirs,tamil nadu"]
        };
        for(let value of object.values(obj)){
            console.log(values);
        }
    }


